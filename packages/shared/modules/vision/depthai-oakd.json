{
  "id": "depthai-oakd",
  "title": "DepthAI OAK-D",
  "description": "DepthAI OAK-D camera support for ROS 2 - provides stereo vision and depth sensing",
  "category": "vision",
  "version": "1.0.0",
  "rosDistros": ["humble", "foxy"],
  "dependencies": [],
  "apt": [
    "ros-humble-depthai-ros",
    "ros-humble-depthai-bridge",
    "ros-humble-depthai-examples",
    "ros-humble-depthai-filters",
    "ros-humble-depthai-ros-msgs"
  ],
  "pip": ["depthai"],
  "env": {
    "DEPTHAI_LEVEL": "info"
  },
  "setupCommands": [],
  "udevRules": [
    "SUBSYSTEM==\"usb\", ATTRS{idVendor}==\"03e7\", ATTRS{idProduct}==\"2150\", MODE=\"0666\"",
    "SUBSYSTEM==\"usb\", ATTRS{idVendor}==\"03e7\", ATTRS{idProduct}==\"2485\", MODE=\"0666\"",
    "SUBSYSTEM==\"usb\", ATTRS{idVendor}==\"03e7\", ATTRS{idProduct}==\"f63b\", MODE=\"0666\""
  ],
  "expose": [11311],
  "bashrcAliases": [
    "alias oakd='ros2 launch depthai_ros_driver camera.launch.py'",
    "alias oakd-rgb='ros2 launch depthai_ros_driver camera.launch.py camera_model:=OAK-D-LITE'"
  ]
}

